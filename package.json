{
  "name": "@maikovskii-danil/ui-framework-vue",
  "version": "3.0.0",
  "prettier": "@maikovskii-danil/prettier-config-vue",
  "type": "module",
  "license": "MIT",
  "author": "maikovskii-danil",
  "main": "dist/index.js",
  "types": "index.d.ts",
  "homepage": "https://github.com/maikovskii-danil/ui-framework-vue#readme",
  "bugs": {
    "url": "https://github.com/maikovskii-danil/ui-framework-vue/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maikovskii-danil/ui-framework-vue.git"
  },
  "scripts": {
    "generate": "node scripts/generate-index.cjs",
    "cleanInstall": "npm clean-install --fund false --audit false",
    "prepackage:build": "run-s cleanInstall generate",
    "package:build": "vite build --config ./configs/vite.config.ts",
    "prelint": "npm run cleanInstall",
    "lint": "npm run lintOnly",
    "lintOnly": "eslint --config ./configs/eslint.config.ts",
    "postlintOnly": "echo 'lint is done'",
    "format": "prettier --write --ignore-unknown src/",
    "prettierCheck": "prettier --check --ignore-unknown src/",
    "prepublishOnly": "npm run package:build",
    "story:build": "histoire build --config ./configs/histoire.config.ts",
    "preview:build": "run-s cleanInstall story:build",
    "prestory:dev": "run-p cleanInstall kill:dev",
    "prestory:preview": "run-p preview:build kill:preview",
    "kill:dev": "npx kill-port 5175",
    "kill:preview": "npx kill-port 5176",
    "story:dev": "histoire dev --config ./configs/histoire.config.ts --port 5175",
    "story:preview": "histoire preview --config ./configs/histoire.config.ts --port 5176",
    "prepare": "husky configs/.husky"
  },
  "peerDependencies": {
    "@vueuse/core": "^13.2.0",
    "tailwindcss": "^4.1.11",
    "vue": "^3.5.16"
  },
  "devDependencies": {
    "@histoire/plugin-vue": "^1.0.0-alpha.2",
    "@maikovskii-danil/eslint-config-js": "^4.0.2",
    "@maikovskii-danil/prettier-config-vue": "^3.0.0",
    "@tailwindcss/vite": "^4.1.10",
    "@types/node": "^22.15.31",
    "@vitejs/plugin-vue": "^5.2.4",
    "eslint": "~9.29.0",
    "eslint-plugin-vue": "~10.2.0",
    "globals": "~16.2.0",
    "histoire": "^1.0.0-alpha.2",
    "husky": "^9.1.7",
    "npm-run-all2": "^8.0.4",
    "prettier": "^3.5.3",
    "tailwindcss": "^4.1.11",
    "typescript": "~5.8.0",
    "typescript-eslint": "~8.34.0",
    "vite": "^6.2.4"
  },
  "engines": {
    "node": "^22.17.0",
    "npm": "^11.4.2"
  },
  "os": [
    "darwin",
    "linux"
  ],
  "devEngines": {
    "runtime": {
      "name": "node",
      "onFail": "error"
    },
    "packageManager": {
      "name": "npm",
      "onFail": "error"
    }
  }
}